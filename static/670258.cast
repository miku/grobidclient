[45.877889, "o", "ogy</orgName>\r\n\t\t\t\t\t\t\t\t<orgName type=\"institution\">Universite ´Libre de Bruxelles</orgName>\r\n\t\t\t\t\t\t\t\t<address>\r\n\t\t\t\t\t\t\t\t\t<postCode>1050</postCode>\r\n\t\t\t\t\t\t\t\t\t<settlement>Brussels</settlement>\r\n\t\t\t\t\t\t\t\t\t<country key=\"BE\">Belgium</country>\r\n\t\t\t\t\t\t\t\t</address>\r\n\t\t\t\t\t\t\t</affiliation>\r\n\t\t\t\t\t\t</author>\r\n\t\t\t\t\t\t<title level=\"a\" type=\"main\">Split Sex Ratios in Perennial Social Hymenoptera: A Mixed Evolutionary Stable Strategy from the Queens&apos; Perspective?</title>\r\n\t\t\t\t\t</analytic>\r\n\t\t\t\t\t<monogr>\r\n\t\t\t\t\t\t<imprint>\r\n\t\t\t\t\t\t\t<date type=\"published\" when=\"2003-09-29\">September 29, 2003</date>\r\n\t\t\t\t\t\t</imprint>\r\n\t\t\t\t\t</monogr>\r\n\t\t\t\t\t<idno type=\"MD5\">2B3AEE7E0FA1C0A0C46A02A2602B14F9</idno>\r\n\t"]
